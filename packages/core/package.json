{
  "name": "@maxgraph/core",
  "license": "Apache-2.0",
  "private": false,
  "version": "0.15.0",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "description": "maxGraph is a fully client side JavaScript diagramming library that uses SVG and HTML for rendering.",
  "keywords": [
    "browser",
    "canvas",
    "diagram",
    "graph",
    "svg"
  ],
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      }
    },
    "./css/*": "./css/*",
    "./images/*": "./images/*",
    "./package.json": "./package.json",
    "./xsd/*": "./xsd/*"
  },
  "files": [
    "css",
    "lib",
    "images",
    "xsd"
  ],
  "homepage": "https://github.com/maxGraph/maxGraph",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxGraph/maxGraph"
  },
  "bugs": {
    "url": "https://github.com/maxGraph/maxGraph/issues"
  },
  "scripts": {
    "clean": "rimraf lib",
    "dev": "tsc --watch",
    "build": "tsc --version && tsc",
    "docs:api": "typedoc src/index.ts",
    "prepack": "run-s clean build",
    "test": "jest --runInBand",
    "test-check": "tsc --noEmit --project tsconfig.test.json"
  },
  "devDependencies": {
    "@swc/core": "~1.10.14",
    "@swc/jest": "~0.2.37",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "npm-run-all": "~4.1.5",
    "rimraf": "~6.0.1",
    "typedoc": "~0.27.6"
  }
}
